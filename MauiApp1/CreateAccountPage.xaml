<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.CreateAccountPage">
    <StackLayout Padding="5">
        <Entry Placeholder="Username" x:Name="UsernameEntry"/>
        <Entry Placeholder="Password" IsPassword="True" x:Name="PasswordEntry"/>
        <Button Text="Create Account" Clicked="OnCreateAccountClicked"/>
    </StackLayout>
</ContentPage>